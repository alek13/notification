<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          http://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">
    <entity name="Shippinno\Notification\Domain\Model\Notification" table="notifications">
        <id name="notificationId" type="notification_id"/>
        <field name="subject" column="subject" type="notification_subject"/>
        <field name="body" column="body" type="notification_body"/>
        <field name="deduplicationKey" column="deduplication_key" type="notification_deduplication_key" nullable="true"/>
        <field name="templateName" column="template_name" nullable="true"/>
        <field name="templateVariables" column="template_variables" nullable="true"/>
    </entity>
</doctrine-mapping>